{
  "name": "dapp-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o -w -dev",
    "build-dev": "ng build",
    "build-noaot": "ng build --aot false --output-path ./build/DappUI --target -prod --env=prod",
    "build": "ng build --aot true --output-path ../../../build/FormDesigner --target -prod --env=prod",
    "build-sm": "ng build --aot true --output-path ../../../build/FormDesigner --target -prod --env=prod -sm",
    "test": "ng test",
    "lint": "ng lint --format prose > lintresult.txt",
    "e2e": "ng e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts **/e2e/*.*' ./app/",
    "typedoc": "typedoc",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat=xlf --outFile=../../../i18n/FormDesigner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "ipfs-mini": "^1.1.2",
    "json-loader": "^0.5.7",
    "rxjs": "^5.5.6",
    "truffle-contract": "^3.0.4",
    "web3": "^1.0.0-beta.30",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.2",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
